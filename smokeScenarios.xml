<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <listeners>
        <listener class-name="com.magento.listener.Listener"></listener>
    </listeners>
    <test thread-count="5" name="Test">
        <groups>
            <run>
                <include name="preTestRuns"/>
                <include name="browserSetup"/>
                <exclude name="userAccounts.*"/>
                <include name="addCart.*"/>
                <include name="postTestRuns"/>
                <include name="finalFinish"/>
            </run>
        </groups>
        <classes>
            <class name="com.magento.browser_setup.BrowserSetup"/>
            <class name="com.magento.AppTest"/>
            <class name="com.magento.UserAccounts"/>
            <class name="com.magento.AddToCart"></class>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->
